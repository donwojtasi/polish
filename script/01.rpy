    # lpdcat surprise_1 "\"It can't be...\" "
    voice voice_file.lpdcat(1,8)
    lpdcat surprise_1 "\"Liao? Co się stało? Czemu jesteś taki zdziwiony?\" "
    lpdcat surprise_1 "\"Niemożliwe...\" "

# game/script/01.rpy:1219
translate polish start_ba3167db:

    # voice voice_file.lpdcat(1,9)
    # lpdcat difficult "\"Wait, can you see me now?\" "
    voice voice_file.lpdcat(1,9)
    lpdcat difficult "\"Czekaj, możesz mnie teraz zobaczyć?\" "

# game/script/01.rpy:1226
translate polish start_2b5f17fe:

    # voice voice_file.tig(1,30)
    # tig frown "\"What are you? Your aura, it's quite strange.\" "
    voice voice_file.tig(1,30)
    tig frown "\"Czym ty jesteś? Twoja aura jest dość dziwna.\" "

# game/script/01.rpy:1228
translate polish start_1eea30a9:

    # avt "\"Uh, so I'm not wrong?\" "
    avt "\"Uch, to jednak mam rację?\" "

# game/script/01.rpy:1230
translate polish start_d0476f20:

    # avt "\"Senpai, you... you're... a cat?\" "
    avt "\"Senpai, ty... ty jesteś... kotem?\" "

# game/script/01.rpy:1236
translate polish start_edcd2773:

    # voice voice_file.lpdcat(1,10)
    # lpdcat serious "\"No, I'm a {font=tl/None/NotoSansCJKtc-Bold.otf}leopard cat{/font}. Even though the name ends with cat, I've got nothing to do with lowly house cats.\" "
    voice voice_file.lpdcat(1,10)
    lpdcat serious "\"Nie, jestem {font=tl/polish/NotoSans-SemiCondensedBold.ttf}kotkiem bengalskim{/font}. Nawet jeśli nazwa zawiera słowo {i}kot{/i}, to nie mam nic wspólnego z potulnymi, domowymi kotami.\" "

# game/script/01.rpy:1238
translate polish start_ede3b769:

    # avt "\"Aren't they basically the same? Leopard cats are just as big as house cats after all...\" "
    avt "\"A nie są one praktycznie takie same? Są przecież tej samej wielkości, co zwykłe dachowce...\" "

# game/script/01.rpy:1240
translate polish start_ad0f903e:

    # voice voice_file.lpdcat(1,11)
    # lpdcat angry "\"Oh, please! I'm a heaps bigger than some house cat!\" "with vpunch
    voice voice_file.lpdcat(1,11)
    lpdcat angry "Och, proszę cię! Jestem o wiele większy od jakiegoś dachowca!\" " with vpunch

# game/script/01.rpy:1242
translate polish start_a4939b12:

    # voice voice_file.lpdcat(1,12)
    # lpdcat angry "\"Putting me on the same level as a house cat is just humiliating!\" "
    voice voice_file.lpdcat(1,12)
    lpdcat angry "\"Wkładanie mnie do tego samego worka z kotem domowym jest upokarzające!\" "

# game/script/01.rpy:1244
translate polish start_03d85f1e:

    # avt "\"That's besides the point!\" "
    avt "\"Odbiegamy od tematu!\" "

# game/script/01.rpy:1246
translate polish start_14a41b6a:

    # avt "\"Senpai... you clearly didn't look like that before!\" "
    avt "\"Senpai... na pewno wcześniej tak nie wyglądałeś!\" "

# game/script/01.rpy:1248
translate polish start_b945aeaa:

    # voice voice_file.lpdcat(1,13)
    # lpdcat normal "\"Nope, I've always looked like this. To most people I'm no more than an ordinary \nstudent.\" "
    # lpdcat normal "\"Hmm... but why can you suddenly see my real form?\" "
    voice voice_file.lpdcat(1,13)
    lpdcat normal "\"Nie, zawsze taki byłem. Dla większości ludzi jestem zwyczajnym studentem.\" "
    lpdcat normal "\"Hmm... Czemu tak nagle możesz zobaczyć moją prawdziwą formę?\" "

# game/script/01.rpy:1250
translate polish start_1bed9db8:

    # voice voice_file.lpdcat(1,14)
    # lpdcat surprise_2"\"Also... those two behind you...\" "
    voice voice_file.lpdcat(1,14)
    lpdcat surprise_2 "\"A poza tym... tych dwóch za tobą...\" "

# game/script/01.rpy:1256
translate polish start_9d48f4bf:

