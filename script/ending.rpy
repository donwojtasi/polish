    # b "...... "
    b "..."

# game/script/ending.rpy:553
translate polish ending_lpd_dead_33cd335f:

    # b "Suddenly, nothing hurts anymore. "
    b "Nagle, wszystko ustaje."

# game/script/ending.rpy:579
translate polish cg_a_day_trip_to_wutai_9cfcd82a:

    # b2 "I woke up somewhere, but not my home.{nw}"
    b2 "Obudziłem się w nieznanym mnie miejscu.{nw}"

