# game/script/ending.rpy:657
translate polish cg_a_day_trip_to_wutai_f6b1d5c1:

    # b2 "Sometimes, I just feel a little lonely, nothing more or nothing less.{nw}"
    b2 "Czasami czuję się jednak samotny, ale nie jest to czymś, czym warto się przejmować.{nw}"

